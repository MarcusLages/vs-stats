{
  "name": "statosaurus",
  "displayName": "Statosaurus",
  "description": "VS Code extension for checking your coding stats. PS: Your data will not be stolen or given overseas.",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [{
          "id": "statosaurus-view",
          "title": "Statosaurus",
          "icon": "./resources/statosaurus.svg"
        }]
    },
    "views": {
      "statosaurus-view": [
        {
          "icon": "./resources/statosaurus.svg",
          "id": "clocksWindow",
          "name": "Clocks"
        },
        {
          "icon": "./resources/statosaurus.svg",
          "id": "heatCommitWindow",
          "name": "Commits"
        },
        {
          "icon": "./resources/statosaurus.svg",
          "id": "lineStatsWindow",
          "name": "Changes"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "extensionDependencies": [
    "vscode.git"
  ],
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.36.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcusLages/vs-stats.git"
  },
  "keywords": [
    "vscode",
    "data-visualization",
    "stats",
    "extension",
    "javascript",
    "python"
  ],
  "author": "Marcus Lages and Ben Nguyen",
  "contributors": [
    {
      "name": "Marcus Lages",
      "email": "marcusvlages@gmail.com",
      "url": "https://github.com/MarcusLages"
    },
    {
      "name": "Ben Nguyen",
      "email": "ben.nguyenxxii@gmail.com",
      "url": "https://github.com/BenNg1"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MarcusLages/vs-stats/issues"
  },
  "homepage": "https://github.com/MarcusLages/vs-stats#readme"
}
